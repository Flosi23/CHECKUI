{
  "$schema": "https://mintlify.com/schema.json",
  "name": "CHECKUI",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.png",
  "colors": {
    "primary": "#063773",
    "light": "#0271C2",
    "dark": "#005EA8",
    "anchors": {
      "from": "#005EA8",
      "to": "#0271C2"
    }
  },
  "topbarCtaButton": {
